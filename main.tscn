[sub_resource type="GDScript" id="GDScript_2cqfq"]
script/source = "extends Sprite2D

@export var blink_speed = 2.0  # How fast it blinks
@export var min_alpha = 0.3  # Minimum transparency (0 = invisible, 1 = solid)
@export var max_alpha = 1.0  # Maximum transparency
@export var distort_amount = 5.0  # How much it moves up/down
@export var enable_distortion = true  # Toggle wave effect on/off

var time_passed = 0.0
var original_position: Vector2

func _ready() -> void:
	original_position = position

func _process(delta: float) -> void:
	time_passed += delta
	
	# Blinking effect (fade in and out)
	var alpha = lerp(min_alpha, max_alpha, (sin(time_passed * blink_speed) + 1.0) / 2.0)
	modulate.a = alpha
	
	# Optional: Distortion/wave effect
	if enable_distortion:
		var offset_y = sin(time_passed * blink_speed * 1.5) * distort_amount
		position.y = original_position.y + offset_y
"

